{
  "name": "massage-appointment",
  "version": "1.0.0",
  "private": true,
  "description": "推拿预约小程序",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "Sass",
    "framework": "React"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:weapp:min": "NODE_ENV=production taro build --type weapp --minify",
    "build:weapp:dev": "taro build --type weapp --sourcemap=false --minify=false",
    "build:h5": "taro build --type h5",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "dependencies": {
    "@tarojs/components": "^4.1.6",
    "@tarojs/helper": "^4.1.6",
    "@tarojs/plugin-framework-react": "^4.1.6",
    "@tarojs/plugin-platform-h5": "^4.1.6",
    "@tarojs/plugin-platform-weapp": "^4.1.6",
    "@tarojs/react": "^4.1.6",
    "@tarojs/runtime": "^4.1.6",
    "@tarojs/shared": "^4.1.6",
    "@tarojs/taro": "^4.1.6",
    "dayjs": "^1.11.14",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "taro-ui": "^3.3.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-react": "^7.22.5",
    "@babel/runtime": "^7.24.0",
    "@tarojs/cli": "^4.1.6",
    "@tarojs/vite-runner": "^4.1.6",
    "@types/jest": "^29.3.1",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.33",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-preset-taro": "^4.1.6",
    "eslint": "^8.57.0",
    "eslint-config-taro": "^4.1.6",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.3.1",
    "postcss": "^8.4.38",
    "sass": "^1.77.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "vite": "^4.5.5"
  }
}
